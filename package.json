{
  "name": "javelina",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:backend\" --names \"frontend,backend\" --prefix-colors \"cyan,magenta\"",
    "build": "next build",
    "build:backend": "cd backend && npm run build",
    "build:full": "npm run build && npm run build:backend",
    "start": "next start",
    "start:backend": "cd backend && npm run start",
    "start:full": "concurrently \"npm run start\" \"npm run start:backend\" --names \"frontend,backend\" --prefix-colors \"cyan,magenta\"",
    "lint": "next lint",
    "postinstall": "cd backend && npm install"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "@stripe/react-stripe-js": "^5.2.0",
    "@stripe/stripe-js": "^8.1.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.74.0",
    "@tanstack/react-query": "^5.64.2",
    "@types/uuid": "^10.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gsap": "^3.13.0",
    "next": "^15.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-easy-crop": "^5.5.3",
    "recharts": "^3.2.1",
    "stripe": "^19.2.0",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "@types/stripe": "^8.0.416",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.4",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  }
}
